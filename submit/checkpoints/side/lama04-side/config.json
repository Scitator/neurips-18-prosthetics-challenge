{
  "shared": {
    "state_size": 344,
    "action_size": 19,
    "history_len": 4,
    "n_step": 1,
    "gamma": 0.99
  },
  "args": {
    "algorithm": "catalyst/rl/algorithms/ecritic_quantile.py"
  },
  "actor": {
    "actor": "LamaActor",
    "hiddens": [
      512,
      512,
      256,
      256
    ],
    "layer_fn": "Linear",
    "bias": false,
    "norm_fn": "LayerNorm",
    "activation_fn": "ReLU",
    "out_activation": "Tanh"
  },
  "critic": {
    "critic": "Critic",
